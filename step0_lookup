# 도로폭 분류 함수
def get_width_class(width):
    ranges = [
        (6, 8, "6m이상8m미만"),
        (8, 10, "8m이상10m미만"),
        (10, 12, "10m이상12m미만"),
        (12, 15, "12m이상15m미만"),
        (15, 20, "15m이상20m미만"),
        (20, 25, "20m이상25m미만"),
        (25, 30, "25m이상30m미만"),
        (30, 35, "30m이상35m미만"),
        (35, 40, "35m이상40m미만"),
        (40, 999, "40m이상")
    ]
    for low, high, label in ranges:
        if low <= width < high:
            return label
    return None

# 교차각 분류 함수
def get_angle_class(angle):
    if 45 <= angle < 75:
        return "60°"
    elif 75 <= angle < 105:
        return "90°전후"
    elif 105 <= angle < 135:
        return "120°"
    return None

# 가각길이 조회 함수
def get_corner_length(angle, w1, w2):
    ac = get_angle_class(angle)
    wc1 = get_width_class(w1)
    wc2 = get_width_class(w2)
    if ac and wc1 and wc2:
        return lookup_table.get((ac, wc1, wc2)) or lookup_table.get((ac, wc2, wc1))
    return None
